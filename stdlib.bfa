#MACRO (\w[\w\d]*)\s*=\s*(-?\d+)->SET $1 $2
#MACRO (\w[\w\d]*)\s*\=\s*([A-Za-z_][\w\d]*)->MOV $2 $1
#MACRO (\w[\w\d]*)\s*\+=\s*(\w[\w\d]*)->ADD $1 $2
#MACRO (\w[\w\d]*)\s*-=\s*(\w[\w\d]*)->SUB $1 $2
#MACRO (\w[\w\d]*)\s*\+=\s*(-?\d+)->INC $1 $2
#MACRO (\w[\w\d]*)\s*-=\s*(\d+)->INC $1 -$2
#MACRO (\w[\w\d]*)\s*-=\s*(-\d+)->INC $1 $2
#MACRO (\w[\w\d]*)\s*\+\+->INC $1 1
#MACRO (\w[\w\d]*)\s*--->INC $1 -1
#MACRO var\s*(\w[\w\d]*)\s*->#MALLOC $1